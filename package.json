{
  "name": "@blackbaud/skyux-visual",
  "version": "0.0.1",
  "description": "Provides a Node.js library for running e2e visual tests",
  "main": "bundles/skyux-visual.umd.js",
  "module": "index.js",
  "engines": {
    "node": ">=6.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackbaud/skyux-visual.git"
  },
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "test": "rimraf dist && tsc --outDir dist && istanbul cover --report=html --report=text-summary ./node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json"
  },
  "keywords": [],
  "author": "Blackbaud, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blackbaud/skyux-visual/issues"
  },
  "homepage": "https://github.com/blackbaud/skyux-visual#readme",
  "dependencies": {
    "@blackbaud/skyux-logger": "1.0.2",
    "pix-diff": "2.0.1",
    "protractor": "5.3.1"
  },
  "devDependencies": {
    "@types/core-js": "0.9.46",
    "@types/jasmine": "2.8.6",
    "@types/node": "9.6.6",
    "core-js": "2.5.5",
    "istanbul": "0.4.5",
    "jasmine": "3.1.0",
    "mock-require": "3.0.2",
    "rewiremock": "3.7.1",
    "rimraf": "2.6.2",
    "tslint": "5.9.1",
    "typescript": "2.8.1"
  }
