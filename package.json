{
  "name": "@blackbaud/skyux-visual",
  "version": "0.0.1",
  "description": "Provides a Node.js library for running e2e visual tests",
  "main": "bundles/skyux-visual.umd.js",
  "module": "index.js",
  "engines": {
    "node": ">=6.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackbaud/skyux-visual.git"
  },
  "scripts": {
    "ci": "npm run test:ci && npm run build",
    "test": "npm run lint && npm run test:unit",
    "test:ci": "npm run test:unit:ci",
    "test:unit": "npm run test:unit:base -- config/karma/karma.conf.js",
    "test:unit:ci": "npm run test:unit:base -- config/karma/karma.conf.js",
    "test:unit:base": "node ./node_modules/karma/bin/karma start",
    "prepare": "node ./scripts/prepare-package",
    "build": "npm run lint && rimraf dist && tsc && node_modules/.bin/webpack --config config/webpack/webpack-prod.config.js",
    "watch": "npm run test:unit -- --auto-watch --no-single-run",
    "lint": "tslint 'src/**/*.ts'"
  },
  "keywords": [],
  "author": "Blackbaud, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blackbaud/skyux-visual/issues"
  },
  "homepage": "https://github.com/blackbaud/skyux-visual#readme",
  "dependencies": {
    "@blackbaud/skyux-logger": "1.0.2",
    "pix-diff": "2.0.1",
    "protractor": "5.3.1"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "@types/node": "9.6.5",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "3.1.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "remap-istanbul": "0.11.0",
    "rimraf": "2.6.2",
    "source-map-inline-loader": "1.0.0",
    "ts-loader": "4.2.0",
    "tslint": "5.9.1",
    "tslint-loader": "3.6.0",
    "typescript": "2.8.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14",
    "webpack-merge": "4.1.2"
  }
}
